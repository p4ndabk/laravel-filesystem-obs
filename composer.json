{
    "name": "pandabk/laravel-filesystem-obs",
    "description": "Huawei Cloud OBS filesystem storage for Laravel 9+",
    "keywords": [
        "huawei",
        "obs",
        "laravel",
        "filesystem",
        "storage"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "illuminate/support": "^9.0|^10.0",
        "league/flysystem": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Obs\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Obs\\Test\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Obs\\ObsServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
